{
  "compilerOptions": { 
    "target": "es2016", 
    "module": "ESNext", 
    "moduleResolution": "node", 
    "baseUrl": ".", 
    "sourceMap": true,
    "paths": {
      "@vue/*": ["packages/*/src"],
      "vue": ["packages/vue/src"]
    }, 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "strict": false, 
    "skipLibCheck": true
  }
}